class MyStack(object):

    def __init__(self):
        self.stack = []      

    def push(self, x):
        self.stack.append(x)

    def pop(self):
        item = self.stack[-1]
        del self.stack[-1]
        return item

    def top(self):
        return self.stack[-1]
        
    def empty(self):
        if len(self.stack) == 0:
            return True
        else: 
            return False